{
    "mongo": {
        "host": "localhost",
        "port": 27017,
        "tz_aware": true,
        "db": "hyperstream"
    },
    "output_path": "output",
    "plugins": [],
    "online_engine": {
        "interval": {
            "start": -60,
            "end": -10
        },
        "sleep": 30,
        "iterations": 50,
        "alarm": 6900
    },
    "online_engine_backup": {
        "interval": {
            "start": -60,
            "end": -10
        },
        "sleep": 5,
        "iterations": 100
    }
}
